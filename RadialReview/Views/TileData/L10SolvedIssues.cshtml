@model long

<div class="review smallBreak2 loading tiny l10-issue-tile" style="min-height:30px">
	<div class="l10-solved-issue-heading heading tile-title" style="@(ViewBag.HeadingStyle)" data-style="@(ViewBag.HeadingStyle)">
		<span class="handle">{{(model.L10SolvedIssues | filter:{KeyId:@(Model)})[0].Title}}&nbsp;</span>
		<span class="tile-buttons">
			<a href="/l10/details/@(Model)#Scorecard"  target="_blank" class="tile-button tile-button-view-all" title="View All"><span class="glyphicon glyphicon-th-large"></span></a>
			@*<a href="javascript:void(0)" class="tile-button tile-button-add-new issuesModal" data-method="CreateIssue" data-showusers="false" data-recurrence="@Model" title="Create New"><span class="glyphicon glyphicon-plus-sign"></span></a>*@
		</span>
	</div><div class="tile">
		@{ Html.RenderPartial("~/Views/Issues/Partial/list.cshtml", new ViewDataDictionary(){
		{"AddHeaders", false},
		{"AddController", false},
		{"UsePopup",true},
		{"AllowEditOwner", false},
		{"RecurrenceId", Model},
		{"Selector", "((model.L10SolvedIssues )| filter:{KeyId:"+Model+"})[0].Contents "},
	}); }<div ng-show="(model.L10SolvedIssues  | filter:{KeyId:@(Model)}).length==0" class="gray alignCenter"  style="padding:16px;" elem-ready="functions.reload((model.L10SolvedIssues | filter:{KeyId:@(Model)}).length==0)">Please wait</div><div ng-if="(model.L10SolvedIssues  | filter:{KeyId:@(Model)})[0].HasError" class="gray alignCenter" style="padding:16px;">{{(model.L10SolvedIssues  | filter:{KeyId:@(Model)})[0].Message}}</div>
</div>
</div>
