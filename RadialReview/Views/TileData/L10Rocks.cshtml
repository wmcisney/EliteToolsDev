<div class="review smallBreak2 loading tiny rock-tile l10-rock-tile" style="min-height:30px">
	<div class="rock-heading heading tile-title" style="@(ViewBag.HeadingStyle)" data-style="@(ViewBag.HeadingStyle)">
		<span class="handle">{{(model.L10Rocks | filter:{KeyId:@(Model)})[0].Title}}&nbsp;</span>
		<span class="tile-buttons">
			<a href="/l10/details/@(Model)#Rocks"  target="_blank" class="tile-button tile-button-view-all" title="View All"><span class="glyphicon glyphicon-th-large"></span></a>
			<a href="javascript:void(0)" class="tile-button tile-button-add-new rockModal" data-controller="L10" data-method="AddRockModal" data-hidemeetings="true" data-meetingId="@Model" title="Add Rock"><span class="glyphicon glyphicon-plus-sign"></span></a>
		</span>
	</div><div class="tile">@{ Html.RenderPartial("~/Views/Rocks/Partial/list.cshtml", new ViewDataDictionary(){
								   {"AddHeaders", false},
								   {"AddController", false},
								   {"AddCompletion", false},
								   {"AddDatePicker", false},
								   {"UsePopup", true},
								   {"RecurrenceId", Model},
								   {"RockName", Html.Organization().NotNull(x => x.Settings.NotNull(y => y.RockName.NotNull(z => z.ToLower())))},
								   {"Selector","(model.L10Rocks | filter:{KeyId:"+(Model)+"})[0].Contents"},
								   {"Filter",""}
							   }); }<div ng-show="(model.L10Rocks | filter:{KeyId:@(Model)}).length==0" class="gray alignCenter" elem-ready="functions.reload((model.L10Rocks | filter:{KeyId:@(Model)}).length==0)">Please wait</div></div>
</div>
