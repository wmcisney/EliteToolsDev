	}

	.footer {
		text-align: center;
		color: #444;
		font-family: cursive;
		opacity: .3;
		padding: 40px;
	}

	.video-header {
		text-align: center;
		font-family: sans-serif;
		color: #444;
		font-size: 20px;
		font-weight: bold;
		margin-bottom: 5px;
	}

	.noselect {
		-webkit-touch-callout: none; /* iOS Safari */
